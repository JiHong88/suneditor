    "node_modules",
    "bower_components"
  ],
  "devDependencies": {
    "@babel/core": "~7.21.3",
    "@octokit/rest": "~20.1.0",
    "@webpack-cli/init": "~1.1.3",
    "babel-loader": "~8.1.0",
    "clean-webpack-plugin": "~0.1.19",
    "codemirror": "~5.61.0",
    "css-loader": "~1.0.1",
    "csstype": "~2.6.13",
    "file-loader": "~2.0.0",
    "html-webpack-plugin": "~3.2.0",
    "jasmine": "~2.99.0",
    "jasmine-core": "~2.99.1",
    "jshint": "~2.13.4",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~2.2.0",
    "karma-jasmine": "~1.1.2",
